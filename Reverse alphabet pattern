import java.util.*;//reverse alphabet pattern
public class MyClass {
    public static void main(String args[]) {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     Character a='Z';
     for(int i=0;i<n;i++)
     {
        for(int j=i+1;j>=1;j--)
     {
        System.out.print(a--);
        
     }
     System.out.println();
     a='Z';
    }
}
}
