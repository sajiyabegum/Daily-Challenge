import java.util.*;//find factors and count
public class MyClass {
    public static void main(String args[]) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int c=0;
    for(int i=1;i<=n;i++)
    {
        if(n%i==0){
        c++;
        System.out.print(i+" ");
        }
    }
        System.out.println(c);
    }
}
