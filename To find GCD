import java.util.*;//gcd
public class MyClass {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int z=1;
        for(int i=1;i<=a;i++)
        {
            if(a%i==0&&b%i==0)
            {
                
              z=i;
            }
        }
       System.out.println(z);  
      
    }
}
