mport java.util.*;//print reverse
public class MyClass {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int arr[]=new int[n];
     for(int i=0;i<n;i++){
         arr[i]=sc.nextInt();
     }
     for(int i=n-1;i>=0;i--){
         //if(arr[i]!=arr[n-2])
         System.out.println(arr[i]);
     }
    }
}
