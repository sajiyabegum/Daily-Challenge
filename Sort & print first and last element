import java.util.*;//sort & print 1st and last element
public class MyClass {
public static void main(String args[]) {
Scanner sc=new Scanner(System.in);  
int n=sc.nextInt();
int[] array = new int[n];  
for(int i=0; i<n; i++)  
{  
array[i]=sc.nextInt();  
}  
Arrays.sort(array);
  System.out.println(array[0]);
  System.out.println(array[n-1]);
}  
}
